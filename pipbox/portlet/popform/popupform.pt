<div style="display:none" tal:condition="view/target_form">
<script>
require(['jquery', 'jquery.cookie'], function($) {
  var popform = {};
  popform.got_cookies = null;
  popform.cookies_enabled = function(uid) {
    if ($.cookie(uid)) {
      return false;
    }
    $.cookie(uid, '1', { expires: 365 });
    return $.cookie(uid) !== null;
  };

  ${view/jqtinit}
});
</script>

    <a tal:attributes="href string:${view/form_url}/fg_embedded_view_p3?show_text=1;
                       id view/uid"
       class="pat-plone-modal"
       data-pat-plone-modal='{"content": ".pfg-embedded", "loadLinksWithinModal": false, "width": "${view/data/width}", "actionOptions": {"displayInModal": false} }'>link</a>
</div>
